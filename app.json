{
  "expo": {
    "name": "Digital Clock",
    "slug": "digital-clock",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": ["expo-router", "expo-screen-orientation"],
    "experiments": {
      "typedRoutes": true
    }
  }
}