{
  "expo": {
    "name": "Digital Clock",
    "slug": "digi-app",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/images/digital-clock-icon.png",
    "splash": {
      "image": "./assets/images/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C3E50"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-screen-orientation"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1b873b59-fc06-4509-8f30-5a68191bd74e"
      }
    },
    "owner": "ndavat",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2C3E50"
      },
       "icon": "./assets/icon.png",
      "package": "com.ndavat.digiapp"
    }
  }
}
